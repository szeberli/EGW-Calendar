{
  "name": "egw_calendar",
  "version": "1.0.0",
  "description": "Anzeigen der n√§chsten Events",
  "main": "main.js",
  "scripts": {
    "package-mac": "electron-packager . EGW_Calendar --overwrite --platform=darwin --arch=x64 --icon=resources/icon.icns --prune=true --out=release-builds/mac",
    "package-win": "electron-packager . EGW_Calendar --overwrite --asar=true --platform=win32 --arch=ia32 --icon=resources/icon.ico --prune=true --out=release-builds/win --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"EGW_Calendar\"",
    "package-linux": "electron-packager . EGW_Calendar --overwrite --asar=true --platform=linux --arch=x64 --icon=resources/icon.ico --prune=true --out=release-builds/linux",
    "start": "electron ."
  },

  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Simon Zeberli",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^1.4.1"
  },
  "dependencies": {
    "jquery": "^3.5.1"
  }
}
